@model List<Category>

<h1>Список категорій</h1>
<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>
				Назва Категорії
			</th>
			<th>
				Порядок Відображення
			</th>
		</tr>
	</thead>
	<tbody>
			@foreach (var obj in Model.OrderBy(u=>u.DisplayOrder))
			{
			<tr>
				<td>
						@obj.Name
				</td>
				<td>
						@obj.DisplayOrder
				</td>
			</tr>
			}
	</tbody>
</table>